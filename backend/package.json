{
  "name": "website-backend",
  "description": "",
  "version": "1.0.0",
  "main": "src/server.ts",
  "dependencies": {
    "dotenv": "^16.0.1",
    "express": "^4.19.2",
    "http-proxy": "^1.18.1"
  },
  "devDependencies": {
